@using System.Linq;
@model MembersListViewModel;
@{
    ViewData["Title"] = "Member";

    Func<object, object> GenerateTableRow(MemberViewModel member) 
    {
        return @<tr>
            <td>@member.Name</td>
        </tr>
    }
}

